{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/admin/login",
      "dest": "app.js"
    },
    {
      "src": "/api/admin/logout",
      "dest": "app.js"
    },
    {
      "src": "/api/admin/check-auth",
      "dest": "app.js"
    },
    {
      "src": "/api/admin-ping-direct",
      "dest": "app.js"
    },
    {
      "src": "/api/test-login",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
